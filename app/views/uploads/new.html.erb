<p> New Video </p>
<%= form_for @upload, url: uploads_path, method: :post do |form| %>
  <p> <%= form.file_field :video %></p>
  <br>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, autofocus: true %>
  </div>
  <br>
  <%= form.submit %>
<% end %>
<%= link_to 'Back', uploads_path %>
